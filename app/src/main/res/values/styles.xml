<resources>

    <!-- Base application theme. -->
    <!-- Base application theme. -->
    <style name="AppTheme" parent="AppTheme.Base">
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="AppTheme.Base" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>
    </style>

    <!--<style name="AppTheme.NoActionBar">-->
        <!--<item name="windowActionBar">false</item>-->
        <!--<item name="windowNoTitle">true</item>-->
    <!--</style>-->

    <!--<style name="ErrorTextView">-->
    <!--<item name="android:padding">@dimen/control_SpecingSmall</item>-->
    <!--<item name="android:textColor">@android:color/black</item>-->
    <!--<item name="android:textSize">14sp</item>-->
    <!--<item name="android:gravity">center</item>-->
    <!--</style>-->

    <!--<style name="PrimaryTextView">-->
    <!--<item name="android:textColor">@color/primary_text</item>-->
    <!--<item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Headline</item>-->
    <!--<item name="android:textSize">@dimen/control_TextMedium</item>-->
    <!--</style>-->

    <style name="ToolBar" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="android:background">@color/primary</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="ToolBarWithImage" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <!--ThemeOverlay.AppCompat.Light-->
    <style name="Popup" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:background">@android:color/white</item>
        <item name="android:textColor">@android:color/black</item>
        <!--<item name="android:fontFamily">Roboto-Regular</item>-->
    </style>

    <style name="EditText" parent="Material.Widget.EditText.Light">
        <item name="cet_spanHeight">32dp</item>
        <item name="cet_spanMaxWidth">150dp</item>
        <item name="cet_spanPaddingLeft">8dp</item>
        <item name="cet_spanPaddingRight">12dp</item>
        <item name="cet_spanTextSize">14sp</item>
        <item name="cet_spanTextColor">@color/accent_light</item>
        <item name="cet_spanBackgroundColor">#FFE0E0E0</item>
        <item name="cet_spanSpacing">4dp</item>
        <item name="et_supportMode">helper</item>
        <item name="et_dividerHeight">2dp</item>
        <item name="et_supportTextErrorColor">@color/accent_light</item>
        <item name="android:textColorHint">@color/grey_800</item>
        <item name="et_dividerErrorColor">@color/accent_light</item>
        <item name="android:textColor">@color/primary_dark</item>
        <item name="et_labelTextColor">@color/grey_700</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="et_labelEnable">true</item>
        <item name="et_labelPadding">0dp</item>
    </style>

    <style name="EditTextToogleButton">
        <item name="android:layout_height">@dimen/tbutton_Height</item>
        <item name="android:layout_width">@dimen/tbutton_Width</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:text">""</item>
        <item name="android:textOff">""</item>
        <item name="android:textOn">""</item>
    </style>

    <style name="CompoundButton">
        <item name="android:textColor">@color/accent_dark</item>
        <item name="rd_rippleColor">@color/accent_light</item>
        <item name="rd_rippleType">touch</item>
        <item name="rd_enable">true</item>
        <item name="rd_inInterpolator">@android:anim/decelerate_interpolator</item>
        <item name="rd_outInterpolator">@android:anim/decelerate_interpolator</item>
        <item name="rd_maskType">rectangle</item>
        <item name="rd_cornerRadius">2dp</item>
        <item name="rd_padding">0dp</item>
        <item name="android:textSize">@dimen/control_TextMedium</item>
        <item name="rd_delayClick">none</item>
    </style>

    <style name="Button" parent="Material.Drawable.Ripple.Touch.Light">
        <item name="android:background">@drawable/button_drawable</item>
        <item name="rd_backgroundColor">@color/accent_dark</item>
        <!--<item name="rd_padding">4dp</item>-->
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Button</item>
        <item name="android:textSize">@dimen/control_TextNormal</item>
        <item name="android:radius">2dp</item>
        <item name="rd_rippleColor">@color/accent_light</item>
        <item name="rd_enable">true</item>
        <item name="rd_cornerRadius">4dp</item>
        <item name="android:layout_marginTop">@dimen/button_marginTop</item>
        <item name="android:layout_marginBottom">@dimen/button_marginBottom</item>
        <item name="android:layout_marginStart">@dimen/button_marginStart</item>
        <item name="android:layout_marginEnd">@dimen/button_marginEnd</item>
        <item name="android:animateOnClick">true</item>
        <!--<item name="android:fontFamily">Roboto-Regular</item>-->
    </style>

    <style name="ProgressDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:minWidth">50dp</item>
        <item name="android:maxWidth">50dp</item>
        <item name="android:maxHeight">50dp</item>
        <item name="android:minHeight">50dp</item>
    </style>

    <style name="ProgressView" parent="@style/Material.Drawable.CircularProgress">
        <item name="cpd_strokeColors">@array/colorArray</item>
        <item name="cpd_strokeSize">4dp</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AppCompactCardView">android.support.v7.CardView</style>

    <style name="FloatingButtonDesignSupport">
        android.support.design.widget.FloatingActionButton</style>

    <style name="CardViewOption" parent="AppCompactCardView">
        <item name="android:layout_height">@dimen/card_MinimumHeight</item>
        <item name="cardCornerRadius">@dimen/card_CornerRadius</item>
        <item name="cardElevation">@dimen/card_Elevation</item>
        <item name="cardBackgroundColor">#65000000</item>
    </style>

    <style name="CardViewNewOption" parent="AppCompactCardView">
        <item name="android:layout_height">@dimen/card_MinimumHeight</item>
        <item name="cardCornerRadius">@dimen/card_CornerRadius</item>
        <item name="cardElevation">@dimen/card_Elevation</item>
        <item name="cardBackgroundColor">@color/card_white</item>
    </style>

    <style name="CardViewCancelledOption" parent="AppCompactCardView">
        <item name="android:layout_height">@dimen/card_MinimumHeight</item>
        <item name="cardCornerRadius">@dimen/card_CornerRadius</item>
        <item name="cardElevation">@dimen/card_Elevation</item>
        <item name="cardBackgroundColor">@color/card_light_green</item>
    </style>

    <style name="PrimaryTextView">
        <item name="android:textColor">@color/primary_text</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Headline</item>
        <item name="android:textSize">@dimen/control_TextMedium</item>
    </style>

    <style name="DefaultCardView" parent="AppCompactCardView">
        <item name="android:layout_height">@dimen/card_MinimumHeight</item>
        <item name="cardCornerRadius">@dimen/card_CornerRadius</item>
        <item name="cardElevation">@dimen/card_Elevation</item>
        <!--<item name="android:layout_marginBottom">@dimen/card_Margin</item>-->
    </style>

    <style name="CardViewTransparent" parent="AppCompactCardView">
        <item name="android:layout_height">@dimen/card_MinimumHeight</item>
        <item name="cardCornerRadius">@dimen/card_CornerRadius</item>
        <item name="cardElevation">@dimen/card_Elevation</item>
        <item name="cardBackgroundColor">@color/card_white</item>
        <item name="android:foreground">@drawable/offer_ripple_effect</item>
    </style>

    <style name="HighLightedTextView">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Caption</item>
        <!--<item name="android:textColor">#795310</item>-->
        <item name="android:textColor">#69470b</item>
        <item name="android:textSize">@dimen/control_TextMedium</item>
    </style>

    <style name="Spinner" parent="Material.Widget.Spinner.Light">
        <item name="cet_spanHeight">32dp</item>
        <item name="cet_spanMaxWidth">150dp</item>
        <item name="cet_spanPaddingLeft">8dp</item>
        <item name="cet_spanPaddingRight">12dp</item>
        <item name="cet_spanTextSize">14sp</item>
        <item name="cet_spanTextColor">@color/primary_dark</item>
        <item name="cet_spanBackgroundColor">#FFE0E0E0</item>
        <item name="cet_spanSpacing">4dp</item>
        <item name="et_supportMode">helper</item>
        <item name="et_supportTextErrorColor">@color/error_text</item>
        <item name="android:textColorHint">@color/primary</item>
        <item name="et_dividerErrorColor">@color/error_text</item>
        <item name="android:textColor">@color/primary_dark</item>
        <item name="et_labelTextColor">@color/primary_dark</item>
        <item name="rd_style">@style/Material.Drawable.Ripple.Wave.Light</item>
        <item name="rd_enable">true</item>
        <item name="rd_delayClick">none</item>
        <item name="spn_popupItemAnimation">@anim/abc_fade_in</item>
        <item name="spn_arrowSwitchMode">true</item>
        <item name="spn_arrowAnimDuration">@android:integer/config_shortAnimTime</item>
        <item name="spn_arrowInterpolator">@android:anim/decelerate_interpolator</item>
    </style>

    <style name="HeaderTextView">
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Display4</item>
        <item name="android:textColor">#795310</item>
        <!--<item name="android:textStyle">bold</item>-->
        <item name="android:textSize">@dimen/control_TextNormal</item>
    </style>

    <style name="DialogTextButton">
        <item name="android:textColor">#009688</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textAppearance">@style/Base.TextAppearance.AppCompat.Button</item>
        <item name="android:paddingStart">@dimen/dialog_button_padding_end</item>
        <item name="android:paddingEnd">@dimen/dialog_button_padding_end</item>
        <item name="android:gravity">center</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
    </style>

    <style name="BusinessInfoCardView" parent="AppCompactCardView">
        <item name="android:layout_height">@dimen/card_MinimumHeight</item>
        <item name="cardCornerRadius">@dimen/card_CornerRadius</item>
        <item name="cardElevation">@dimen/card_Elevation</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="cardBackgroundColor">@color/offwhite</item>
    </style>

</resources>
